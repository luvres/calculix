FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     rhoCentralFoam;

//startFrom       startTime;
startFrom       latestTime;

startTime       0;

//stopAt          writeNow
stopAt          endTime;

endTime         0.2;

deltaT          1e-04;

writeControl    runTime;

writeInterval   1e-03;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           0.1;

maxDeltaT       1;


